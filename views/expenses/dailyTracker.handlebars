<div id="expense-wrapper">
  <h1 class="budgetHead">Daily Expense Tracker </h1>
  <div class="container">
  <h1> Expenses </h1>
  	<div class="form-group form-groupMonth">
  	  <label for="month">Month:</label>
  	  <select id='month' class='form-control'>
        <option value="january">January</option>
        <option value="february">February</option>
        <option value="march">March</option>
        <option value="april">April</option>
        <option value="june">June</option>
        <option value="july">July</option>
        <option value="august">August</option>
        <option value="september">September</option>
        <option value="october">October</option>
        <option value="november">November</option>
        <option value="december">December</option>
      </select>
  	</div>
    <div class="form-group form-groupCatergory">
      <label for="catergory">Caterogory:</label>
      <select id='catergory' class='form-control'>
        <option value="food">Food</option>
        <option value="clothing">Clothing</option>
        <option value="utilities">Utilities</option>
        <option value="mortgageRent">Mortgage/Rent</option>
        <option value="fund">Fund</option>
        <option value="medical">Medical</option>
        <option value="insurance">Insurance</option>
        <option value="transportation">Transportation</option>
        <option value="personal">Personal</option>
        <option value="debt">Debt</option>
      </select>
    </div>
  	<div class="form-group">
  	  <label for="expense">Expense Name:</label>
  	  <input type="text" class="form-control" id="expense">
  	</div>
    <div class="form-group">
      <label for="expenseAmount">Amount Spent</label>
      <input type="number" class="form-control" id="expenseAmount">
    </div>
    <button id="budgetBtn" type="button" class="btn btn-default bgtBtn">Submit</button>
  </div>